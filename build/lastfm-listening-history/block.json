{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "lastfm-listening-history/lastfm-listening-history",
  "version": "0.1.0",
  "title": "Lastfm Listening History",
  "category": "widgets",
  "icon": "format-audio",
  "description": "Display recent tracks from Last.fm",
  "supports": {
    "html": false
  },
  "attributes": {
    "apiKey": {
      "type": "string",
      "default": ""
    },
    "username": {
      "type": "string",
      "default": ""
    },
    "limit": {
      "type": "number",
      "default": 5
    }
  },
  "textdomain": "lastfm-listening-history",
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "style": "file:./style-index.css",
  "render": "file:./render.php",
  "viewScript": "file:./view.js"
}